{
  "apiVersion": 3,
  "name": "acf/testimonial",
  "title": "Testimonial",
  "description": "A customizable testimonial block with quote, author, and rating.",
  "category": "acf-blocks",
  "icon": "format-quote",
  "keywords": ["testimonial", "review", "quote", "customer"],
  "acf": {
    "renderTemplate": "testimonial-block.php",
    "blockVersion": 3
  },
  "supports": {
    "align": ["wide", "full"],
    "mode": true,
    "jsx": true,
    "anchor": true
  },
  "styles": [
    {
      "name": "default",
      "label": "Default",
      "isDefault": true
    },
    {
      "name": "dark",
      "label": "Dark"
    },
    {
      "name": "card",
      "label": "Card"
    },
    {
      "name": "minimal",
      "label": "Minimal"
    },
    {
      "name": "bubble",
      "label": "Speech Bubble"
    }
  ],
  "example": {
    "attributes": {
      "mode": "preview",
      "data": {
        "acf_testimonial_quote": "This plugin has transformed how we build our WordPress sites. The blocks are beautifully designed and incredibly easy to use.",
        "acf_testimonial_author_name": "Jane Smith",
        "acf_testimonial_author_title": "Product Manager at TechCorp",
        "acf_testimonial_rating": 5
      }
    }
  },
  "style": "file:./testimonial.css",
  "editorStyle": "file:./testimonial.css"
}
